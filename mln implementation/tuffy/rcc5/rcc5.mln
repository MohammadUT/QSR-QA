rcc(region, region, rel!)

rcc(a,b,DR) => rcc(b,a,DR).
rcc(a,b,PO) => rcc(b,a,PO).
rcc(a,b,PP) => rcc(b,a,PPI).
rcc(a,b,EQ) => rcc(b,a,EQ).
rcc(a,b,PPI) => rcc(b,a,PP).

rcc(a,b,rel), rcc(b,c,EQ) => rcc(a,c,rel).
rcc(a,b,DR), rcc(b,c,PPI) => rcc(a,c,DR).
rcc(a,b,PO), rcc(b,c,PP) => rcc(a,c,PO) v rcc(a,c,PP).
rcc(a,b,PP), rcc(b,c,PP) => rcc(a,c,PP).


